{
  "manifest_version": 2,
  "name": "Instant Ratings for Google Moviesâ„¢",
  "version": "0.1",
  "icons": {
    "128": "icon_128.png"
  },
  "description": "This extension adds IMDB ratings to Google Movies movie listings.",
  "permissions": [
    "<all_urls>","webRequest"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": [
      "*://google.co*"
    ],
    "js": ["jquery-3.1.1.min.js", "content.js"]
  }]
}
